<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>


<Pane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cn.novisfff.raspberry.views.SysInfoView">
    <ImageView fitHeight="120.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
        <image>
            <Image url="@/pic/right.png"/>
        </image>
    </ImageView>
    <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="120.0"
                prefWidth="500.0" style="-fx-background-color: transparent;">
        <children>
            <GridPane layoutX="120.0" prefHeight="120.0" prefWidth="120.0">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                </rowConstraints>
                <children>
                    <Pane fx:id="cpu0Pane" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="50.0"
                          prefWidth="50.0">
                        <padding>
                            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                        </padding>
                        <GridPane.margin>
                            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                        </GridPane.margin>
                    </Pane>
                    <Pane fx:id="cpu1Pane" prefHeight="50.0" prefWidth="50.0" GridPane.columnIndex="1">
                        <GridPane.margin>
                            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                        </GridPane.margin>
                    </Pane>
                    <Pane fx:id="cpu2Pane" prefHeight="50.0" prefWidth="50.0" GridPane.rowIndex="1">
                        <GridPane.margin>
                            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                        </GridPane.margin>
                    </Pane>
                    <Pane fx:id="cpu3Pane" prefHeight="50.0" prefWidth="50.0" GridPane.columnIndex="1"
                          GridPane.rowIndex="1">
                        <GridPane.margin>
                            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                        </GridPane.margin>
                    </Pane>
                </children>
            </GridPane>
            <Label alignment="TOP_LEFT" prefHeight="8.0" prefWidth="32.0" text="CPU" textFill="#fcc601" wrapText="true"
                   AnchorPane.leftAnchor="5.0" AnchorPane.topAnchor="5.0">

            </Label>
            <Label text="MB" textFill="WHITE" AnchorPane.leftAnchor="308.0" AnchorPane.topAnchor="80.0">

            </Label>
            <Label alignment="CENTER" layoutX="379.0" layoutY="41.0" prefHeight="20.0" prefWidth="100.0" text="CPU温度"
                   textAlignment="CENTER" textFill="#fcc601" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">

            </Label>
            <Pane fx:id="cpuPane" prefHeight="120.0" prefWidth="120.0">
                <padding>
                    <Insets bottom="10.0" left="10.0"/>
                </padding>
            </Pane>
            <Pane fx:id="memoryPane" layoutX="241.0" prefHeight="110.0" prefWidth="110.0" AnchorPane.bottomAnchor="5.0"
                  AnchorPane.leftAnchor="260.0" AnchorPane.topAnchor="5.0"/>
            <Pane fx:id="temperaturePane0" layoutX="346.0" layoutY="-70.0" prefHeight="100.0" prefWidth="100.0"
                  AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="5.0"/>
            <Label layoutX="380.0" layoutY="17.0" text="Memory" textFill="#fcc601" AnchorPane.leftAnchor="293.0"
                   AnchorPane.topAnchor="30.0">

            </Label>
        </children>
    </AnchorPane>
</Pane>


